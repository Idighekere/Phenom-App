{
  "expo": {
    "name": "Phenom",
    "slug": "Phenom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/phenomicon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/phenomicon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#3B1254"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7d29dd6c-bf84-4b44-af58-e53cf7cfe525"
      }
    }
  }
}
